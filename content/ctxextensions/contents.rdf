		<RDF:li resource="chrome://browser/content/browser.xul"/>
	</RDF:Seq>

	<!-- Firefox -->
	<RDF:Seq about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://ctxextensions/content/contentAreaContextOverlay.xul</RDF:li>
		<RDF:li>chrome://ctxextensions/content/globalOverlay.xul</RDF:li>
		<RDF:li>chrome://ctxextensions/content/navigatorOverlay.xul</RDF:li>
		<RDF:li>chrome://ctxextensions/content/tasksOverlay.xul</RDF:li>
	</RDF:Seq>


</RDF:RDF>
