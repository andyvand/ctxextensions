<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!-- list all the packages being supplied by this jar -->
	<RDF:Seq about="urn:mozilla:package:root">
		<RDF:li resource="urn:mozilla:package:ctxextensions"/>
	</RDF:Seq>

	<!-- package information -->
	<RDF:Description about="urn:mozilla:package:ctxextensions"
		chrome:name="ctxextensions"
		chrome:displayName="ContextMenu Extensions"
		chrome:author="SHIMODA Hiroshi"
		chrome:authorURL="http://piro.sakura.ne.jp/"
		chrome:settingsURL="chrome://ctxextensions/content/pref/prefDialogCall.xul"
		chrome:extension="true"
		chrome:description="Adds many useful features to the browser."/>


	<!-- overlay information -->
	<RDF:Seq about="urn:mozilla:overlays">
